#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
int n,r,c;
    scanf("%d",&n);
    for(r=1;r<=n;r++){
        for(c=1;c<=r;c++){
            printf("* ");
        }
        printf("\n");
    }
    for(r=1;r<n;r++){
        for(c=1;c<=n/3;c++){
            printf("* ");
        }
        printf("\n");
        }
    return 0;
}
